<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="375.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreen">
   <children>
      <Button fx:id="MainScreenRecords" layoutX="122.0" layoutY="83.0" mnemonicParsing="false" onMouseClicked="#GoToRecords" text="Customer Records">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="MainScreenReports" layoutX="162.0" layoutY="198.0" mnemonicParsing="false" onMouseClicked="#GoToReports" text="Reports">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="MainScreenLogout" layoutX="165.0" layoutY="259.0" mnemonicParsing="false" onMouseClicked="#Logout" text="Logout">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="MainScreenLabel" alignment="CENTER" layoutY="23.0" prefHeight="45.0" prefWidth="410.0" text="Welcome $username" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
