<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="490.0" prefWidth="401.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UpdateCustomer">
   <children>
      <Label fx:id="UpdateCustomerLabel" alignment="TOP_LEFT" layoutX="124.0" layoutY="23.0" text="Update Customer">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="UpdateCustomerID" editable="false" layoutX="127.0" layoutY="80.0" />
      <TextField fx:id="UpdateCustomerName" layoutX="126.0" layoutY="131.0" />
      <TextField fx:id="UpdateCustomerAddress" layoutX="126.0" layoutY="184.0" />
      <TextField fx:id="UpdateCustomerPost" layoutX="126.0" layoutY="235.0" />
      <TextField fx:id="UpdateCustomerPhone" layoutX="127.0" layoutY="287.0" />
      <Label layoutX="167.0" layoutY="63.0" text="Customer ID" />
      <Label layoutX="162.0" layoutY="114.0" text="Customer Name" />
      <Label layoutX="184.0" layoutY="167.0" text="Address" />
      <Label layoutX="174.0" layoutY="218.0" text="Postal Code" />
      <Label layoutX="165.0" layoutY="270.0" text="Phone Number" />
      <Button fx:id="UpdateCustomerSaveButton" layoutX="207.0" layoutY="425.0" mnemonicParsing="false" onMouseClicked="#UpdateCustomerSave" text="Save" />
      <Button fx:id="UpdateCustomerCancelButton" layoutX="254.0" layoutY="425.0" mnemonicParsing="false" onMouseClicked="#UpdateCustomerCancel" text="Cancel" />
      <Label layoutX="179.0" layoutY="318.0" text="Country" />
      <Label layoutX="180.0" layoutY="370.0" text="Division" />
      <ComboBox fx:id="UpdateCustomerDivision" layoutX="126.0" layoutY="387.0" prefWidth="150.0" />
      <ComboBox fx:id="UpdateCustomerCountry" layoutX="127.0" layoutY="335.0" onAction="#CountrySelectedUpdate" prefWidth="150.0" />
   </children>
</AnchorPane>
