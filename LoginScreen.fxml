<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreen">
   <children>
      <Label fx:id="LoginTitle" alignment="CENTER" contentDisplay="CENTER" layoutY="14.0" prefHeight="30.0" prefWidth="303.0" text="Scheduling Application" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="LoginUserLabel" alignment="CENTER" layoutX="-2.0" layoutY="57.0" prefHeight="20.0" prefWidth="303.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="LoginPassLabel" alignment="CENTER" layoutY="118.0" prefHeight="20.0" prefWidth="303.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="LoginUsername" layoutX="78.0" layoutY="77.0" promptText="Enter Username" />
      <TextField fx:id="LoginPassword" layoutX="77.0" layoutY="138.0" promptText="Enter Password" />
      <Button fx:id="LoginButton" contentDisplay="CENTER" layoutX="102.0" layoutY="185.0" mnemonicParsing="false" onMouseClicked="#Login" prefHeight="25.0" prefWidth="102.0" text="Login" />
      <TextField fx:id="LoginLocation" editable="false" layoutX="140.0" layoutY="250.0" />
      <Label fx:id="LoginLocationLabel" layoutX="192.0" layoutY="233.0" text="Location" />
   </children>
</AnchorPane>
